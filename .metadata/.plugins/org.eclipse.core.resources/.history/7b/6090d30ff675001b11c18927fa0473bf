package com.example.java;

import com.example.java.model.*;

import java.util.List;

import com.example.java.model.Olive;

public class OlivePress {

	public int getOil(List<Olive> olives) {

		int totalOil = 0;
		for (Olive o : olives) {
			totalOil += o.crush();
		}

		return totalOil;

	}

}
