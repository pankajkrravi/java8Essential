
public class Loops {
	public static void main(String[] args) {
		String monts[] = { "january", "feburary", "March", "April", "may", "June", "July", "August", "Sepetember",
				"october", "November", "December" };
		//For Loop
		for (int i = 0; i < monts.length; i++) {
			System.out.println(monts[i]);
		}
	}
}
